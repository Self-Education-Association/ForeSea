@model List<Manager.Models.ManagerTableHelper.ManageTable>
@{
    ViewBag.Title = "值班表试算";
}

<h2>@ViewBag.Title<span class="label label-warning">错误</span></h2>

@if (Model.Count == 0)
{
    <h3>无可用的值班表，请检查可值班时间表。</h3>
}

@foreach (var item in Model)
{
    <div class="container">
        @Html.Partial("ManageTable", item)
    </div>
}


