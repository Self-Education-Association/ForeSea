@model IEnumerable<LST.Models.TestRecord>

@{
    ViewBag.Title = "View";
}

<h2>View</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            场次
        </th>
        <th>
            开始报名时间
        </th>
        <th>
            结束报名时间
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Score)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @item.Match.Name
        </td>
        <td>
            @item.Match.StartTime
        </td>
        <td>
            @item.Match.EndTime
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Score)
        </td>
        <td>
            @if (item.Match.Enabled)
            {
                Html.ActionLink("取消报名", "Quit", new { id = item.Id });
            }
        </td>
    </tr>
}

</table>
