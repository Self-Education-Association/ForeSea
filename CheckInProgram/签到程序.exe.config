<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0,Profile=Client" />
    </startup>
    <appSettings>
      <!--版本号-->
        <add key="Version" value ="v1.1.0" />
      <!--主界面标题-->
        <add key="Name" value ="大学英语听力课程上课签到" />
      <!--作者信息/授权信息-->
        <add key="Author" value ="Self Education Association" />
      <!--界面底部文字-->
        <add key="Powered" value ="本程序由Self Education Association技术支持" />
      <!--链接文字-->
        <add key="LinkLabel" value ="点击本链接了解更多大学英语听力课程学习信息" />
      <!--链接地址-->
        <add key="LinkUrl" value ="http://www.uibesea.org" />
      <!--是否显示更新信息，1是 0否-->
        <add key="ShowUpdate" value ="1" />
      <!--更新信息详情-->
        <add key="UpdateString" value ="2015-10-25 v2.0.0\n\r重大更新:\n\r重构了签到程序的内部逻辑，优化了内部判断（试验中）。\n\r添加了管理员管理的模块。\n\r添加了分流系统的模块（试运行）。" />
      <!--是否显示弹窗，1是 0否-->
        <add key="ShowPage" value ="0" />
      <!--弹窗标识符-->
        <add key="PageLabel" value ="外语自学与研究中心SEA团队网站改版,学习论坛功能业已同期上线!\n\rTAI春季招生火热进行中，请查看！" />
      <!--弹窗地址-->
        <add key="PageUrl" value ="http://www.uibesea.org" />
      <!--发送心跳时不弹出检查选项的概率-->
        <add key="UnCheckProbability" value ="90" />
      <!--发送心跳的频率-->
        <add key="KeepFrequency" value ="1" />
      <!--检查选项的超时时间-->
        <add key="OverTime" value ="10" />
    </appSettings>
</configuration>